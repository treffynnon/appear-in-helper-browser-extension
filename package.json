{
  "name": "appear-in-meeting-room-helper-browser-extension",
  "repository": "git@github.com:treffynnon/appear-in-helper-browser-extension.git",
  "author": "Simon Holywell <treffynnon@php.net>",
  "version": "1.0.1",
  "description": "Auto enter appear.in meetings muted and cam off (configurable).",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.5.1",
    "yarn": ">=1.3.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.54",
    "@types/jest": "^21.1.8",
    "@types/text-encoding": "^0.0.32",
    "awesome-typescript-loader": "^3.4.1",
    "chalk": "^2.3.0",
    "chrome-location": "^1.2.1",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "mv": "^2.1.1",
    "on-build-webpack": "^0.1.0",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.4",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1",
    "write-file-webpack-plugin": "^4.2.0",
    "zip-webpack-plugin": "^2.1.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack -w --config webpack.dev.js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx,js}' '*.{ts,tsx,js}'",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.[tj]sx?$"
  }
}
